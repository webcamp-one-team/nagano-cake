<div class="row">
  <div class="col-xs-12">
    <h2>会員一覧</h2>
    <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th>会員ID</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th>ステータス</th>
        </tr>
      </thead>
      <tbody>
        <% @members.each do |member| %>
        <tr>
          <th><%= member.id %></th>
          <th><%= link_to "#{member.last_name} #{member.first_name}", admins_member_path(member.id)%></th>
          <th><%= member.email %></th>
          <% if member.is_deleted == true %>
          <th>退会済</th>
          <% else %>
          <th>有効</th>
          <% end %>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>