<div class="container">
  <div class="row">
    <div class="col-xs-12">

      <h2>管理者ログイン</h2>

      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="field">
        <%= f.label :email, "メールアドレス" %>
        <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
      </div>

      <div class="field">
        <%= f.label :password, "パスワード" %>
        <%= f.password_field :password, autocomplete: "current-password" %>
      </div>

      <div class="actions">
        <%= f.submit "ログイン", class: "btn btn-primary" %>
      </div>
      <% end %>
      <%= link_to "会員の方はこちら", new_member_session_path, class: "btn btn-lg" %>
      <div class="initial_admins">
        <h1>初期登録Admins</h1>
        <table>
          <tr>
            <th>メールアドレス</th>
            <th>パスワード</th>
          </tr>
          <tr>
            <td>j@j</td>
            <td>jjjjjj</td>
          </tr>
          <tr>
            <td>k@k</td>
            <td>kkkkkk</td>
          </tr>
          <tr>
            <td>j@j</td>
            <td>kkkkkk</td>
          </tr>
        </table>
      </div>
    </div>
  </div>
</div>