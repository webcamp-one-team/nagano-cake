<!--配送先編集-->
<div id="wrapper">
	<div class="row">
		<div class="col-xs-1"></div>
		<div class="col-xs-10">
			<h4>配送先編集</h4>
			<%= form_for(@address) do |f| %>
			<p>郵便番号</p>
			<%= f.text_field :postal_code %>
			<p>住所</p>
			<%= f.text_field :address %>
			<p>宛名</p>
			<%= f.text_field :name %>
			<%= f.submit "編集する", class:"btn btn-default" %>
			<% end %>
		</div>
		<div class="col-xs-1"></div>
	</div>
</div>