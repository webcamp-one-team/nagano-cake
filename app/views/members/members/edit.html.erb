<div class="container" >
	<div class="row">
		<div class="col-xs-1"></div>
			<div class="col-xs-11">
				<h2><strong>登録情報編集</strong></h2>
					<%= form_with model:@member, url:members_path(@member.id) do |f| %>
  						<table>
						    <tr>
						  	  <th><%= "名前" %></th>
						  	  	<th><%= "(姓)" %></th><td><%= f.text_field :last_name, class:"form-control" %></td>
					　　　     	<th><%= "(名)" %></th><td><%= f.text_field :first_name, class:"form-control" %></td>
						    </tr>
							  <tr>
							    <th><%= "フリガナ" %></th>
							      <th><%= "(セイ)" %></th>
							       <td><%= f.text_field :last_name_kana, class:"form-control" %></td>
						　　　     <th><%= "(メイ)" %></th>
						　　　  　   <td><%= f.text_field :first_name_kana, class:"form-control" %></td>
							  </tr>
							  <tr>
							    <th><%= "メールアドレス" %></th>
							    <th><%= "" %></th>
							      <td><%= f.text_field :email, class:"form-control" %></td>
							  </tr>
							  <tr>
							  	<th><%= "郵便番号(ハイフンなし)" %></th>
							  	<th><%= "" %></th>
							      <td><%= f.text_field :postal_code, class:"form-control" %></td>
							  </tr>
							  <tr>
							    <th><%= "住所" %></th>
							    <th><%= "" %></th>
							      <td><%= f.text_field :address, class:"form-control" %></td>
							  </tr>
							  <tr>
							    <th><%= "電話番号(ハイフンなし)" %></th>
							    <th><%= "" %></th>
							      <td><%= f.text_field :telephone_number, class:"form-control" %></td>
							  </tr>
							  <tr>
					        <th><%= f.submit "変更を保存", class:"btn btn-primary" %></th>
					        <th><%= link_to "退会する", members_unsubscribe_path, class:"btn btn-danger" %></th>
					      </tr>
					  </table>
					<% end %>
			</div>
	</div>
</div>